// Template

#include <iostream>
#include <string> // string

using namespace std;

template <class T> // template <typename T>
T DoubleIt (T x)
{
    return x + x;
}

int main() 
{
   
    cout << "Double 5 = " <<DoubleIt(5)<< "\n"; //int 
    cout << "Double 1.5 = " <<DoubleIt(1.5)<< "\n"; //double 
    cout << "Double true = " << boolalpha << DoubleIt(true) << "\n"; 
    
    string s = "World";
    cout << "Double string \" World \" = " << DoubleIt(s) << "\n";

  return 0;
}    
